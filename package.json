{
  "name": "node-perf-timer",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watchAll",
    "show:cov": "open coverage/lcov-report/index.html",
    "precommit": "lint-staged",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "prettier --write --single-quote --trailing-comma=all",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 84,
        "functions": 90,
        "lines": 95
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkrem/node-perf-timer.git"
  },
  "author": "Ben Kremer <ben.kremer@hotmail.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkrem/node-perf-timer/issues"
  },
  "homepage": "https://github.com/bkrem/node-perf-timer#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.1.0"
  }
}
